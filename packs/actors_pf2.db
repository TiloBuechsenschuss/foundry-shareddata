e":""},"location":{"value":"TC329oR1FaDZSsiD"},"hasCounteractCheck":{"value":false},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightening":{"type":"interval","interval":1,"damage":{"0":"2"}}},"ownership":{"default":0,"u0QnuKi1BChalBsY":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"wqskyPCCfUIwtlhN","name":"Counter Performance","type":"spell","img":"systems/pf2e/icons/spells/counter-performance.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.WILXkjU5Yq3yw10r"}},"system":{"description":{"value":"<p><strong>Trigger</strong> You or an ally within 60 feet rolls a saving throw against an auditory or visual effect.</p>\n<hr />\n<p>Your performance protects you and your allies. Roll a Performance check for a type you know: an auditory performance if the trigger was auditory, or a visual one for a visual trigger. You and allies in the area can use the better result between your Performance check and the saving throw.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["bard","composition","mental"],"rarity":"uncommon","custom":""},"rules":[],"slug":"counter-performance","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-05T21:21:38.469+02:00","version":{"schema":0.793,"foundry":"10.286","system":"4.2.2"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"focus"},"traditions":{"value":[]},"school":{"value":"enchantment"},"components":{"focus":false,"material":false,"somatic":false,"verbal":false},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"60","areaType":"emanation"},"time":{"value":"reaction"},"duration":{"value":""},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"loca